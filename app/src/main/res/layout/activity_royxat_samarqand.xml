<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RoyxatBukhara">
    <Toolbar
        android:title="@string/samarqand"
        android:id="@+id/toolbarSamarkand"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_width="match_parent"
        android:layout_height="50dp" />
    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/tableSamarqand"
        tools:listitem="@layout/korinish_list_item"
        android:layout_width="match_parent"
        app:layout_constraintTop_toBottomOf="@id/toolbarSamarkand"
        android:layout_height="wrap_content"/>
</androidx.constraintlayout.widget.ConstraintLayout>